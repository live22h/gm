
<div class="grid-y">
  <div class="cell small-5" id="main-header">
    <div class="grid-x">
      <div class="cell small-12 medium-3 medium-offset-9">
        <%= render file: 'template/login-form' %>
      </div>
      <div class="cell small-12">
          <div class="logo">
            <%= inline_svg "logo.svg" %>
          </div>
      </div>
      <div class="cell small-12 medium-8 medium-offset-2 search">
        <%= render file: 'template/search.html.erb'%>
      </div>
  </div>
  <div class="cell small-7" id="main-content">
    <div class="grid-x">
    <% @last_orders.each do |order| %>
      <div class="cell small-12 medium-3">
        <% @order_preview = order %>
        <%= render file: 'template/order-preview' %>
      </div>
    <% end %>
  </div>
  </div>
</div>
